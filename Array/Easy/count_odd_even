def count_odd_even(arr):
    odd_count = 0
    even_count = 0
    for i in range(len(arr)):
        if arr[i]%2 == 0:
            even_count += 1
        else:
            odd_count += 1
            
    print("The odd count is:",odd_count)
    print("The even count is:",even_count)

count_odd_even([4,2,19,10,11,5])
            
